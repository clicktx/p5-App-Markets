% layout 'admin-default';
% title __('Edit Category');

<%= link_to 'back to categories' => 'RN_admin_category' %>

<h1><%= title %></h1>
<%= form_for 'RN_admin_category_edit' => ( method => 'POST' ) => begin %>
    <div class="">
        <%= form_field 'admin-category#title' %>
        <%= form_label %>
        <%= form_widget %>
        <%= form_error %>
    </div>
    <%= csrf_field %>
    <%= submit_button 'Save' %>
<% end %>
